{
  "expo": {
    "name": "Keiros Service App",
    "slug": "keirosservice",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.keiros.serviceapp"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.keiros.serviceapp",
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN"
      ]
    },
    "web": {},
    "plugins": [
      "expo-router",
      "expo-build-properties"
    ],
    "scheme": "keiros-service-app",
    "extra": {
      "eas": {
        "projectId": "8657b0f9-0d7a-43d7-a32b-c34a8e262aef"
      },
      "router": {
        "origin": false
      }
    }
  }
}
